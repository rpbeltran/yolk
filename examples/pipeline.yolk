# cat hello.txt | grep (1 + 2) | filter $ @condition_func
PIPELINE begin
PUSH_STR "hello.txt"
PUSH_STR "cat"
EXEC 1
PIPELINE next
PUSH_STR "grep"
PUSH_NUM 2
PUSH_NUM 1
BINOP add
EXEC 1
PIPELINE end
PRINT
# DEBUG_ONLY assert_stack_empty




# Weird nested PIPELINE
# say a | (`say b` | `say c`) | say d

PIPELINE begin
PUSH_STR "a"
PIPELINE next
PIPELINE begin
PUSH_STR "b"
PIPELINE next
PUSH_STR "c"
PIPELINE end
PIPELINE next
PUSH_STR "d"
PIPELINE end
PRINT